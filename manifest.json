{
  "manifest_version": 3,
  "name": "Zoho CRM Data Extractor",
  "version": "1.0",
  "permissions": ["storage", "tabs", "scripting"],
  "host_permissions": [
    "https://crm.zoho.com/*",
    "https://crm.zoho.in/*"
  ],
  "background": {
    "service_worker": "service-worker.js"
  },
  "action": {
    "default_popup": "dist/popup.html"
  }
}
